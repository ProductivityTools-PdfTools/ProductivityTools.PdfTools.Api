[Unit]
Description=PdfTools Service
After=network.target

[Service]
Type=simple
User=springuser
Group=springuser
WorkingDirectory=/opt/PT.PdfTools
# EnvironmentFile=/etc/default/pdftools
ExecStart=/usr/bin/java -jar /opt/PT.PdfTools/pdftools-0.0.1-SNAPSHOT.jar
Restart=on-failure
RestartSec=5
SuccessExitStatus=143
LimitNOFILE=65536

[Install]
WantedBy=multi-user.target
